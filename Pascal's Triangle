vector<vector<int>> mat;
        
        for(int i=0;i<5;i++){
            for(int j=0;j<(i+1);j++){
                if(j==0){
                    mat[i].push_back(1);
                }
                else if(j==(i-1)){
                    mat[i].push_back(1);
                }
                else{
                    int t ;
                    t = mat[i-1][j-1] + mat[i-1][j];
                    mat[i].push_back(t);
                }
            }
        }
        
        return mat;
